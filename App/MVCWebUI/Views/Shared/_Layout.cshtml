@model IList<MVCWebUI.Models.ModuleInfo>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/ui-lightness/jquery-ui-1.10.3.custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/easy/themes/default/easyui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/easy/themes/icon.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/GeneralStyle.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.10.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.3.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/easyui/jquery.easyui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/generalScripts.js")" type="text/javascript"></script>

    @RenderSection("scripts", required: false)
</head>
<body>
    <div id="header">
        
    </div>
    <div id="navigationbar">
        <div id="navigationmenu">
            <ul>
                <li><a href='#'>Home</a></li>
                <li><a href='#'>Material</a></li>
                <li><a href='#'>Speaker</a></li>
                <li><a href='#'>Workflow</a></li>
                <li><a href='#'>HR</a></li>
                <li><a href='#'>Report</a></li>
                <li><a href='#'>Search</a></li>
                <li><a href='#'>Settings</a></li>
                <li><a href='#'>Admin</a></li>
                <li><a href='#'>Others</a></li>
            </ul>
        </div>
        <div id="userprofilepanel">
        </div>
    </div>
    <div id="mainbody" class="easyui-layout">
        <div id="bodyleft" data-options="region:'west', split:true" title="west">
        </div>
        <div id="bodyright" data-options="region:'center', title:'Center'">
            @RenderBody()
        </div>
    </div>
    <div id="footer">
    </div>
</body>
</html>
